<h1>Questions</h1>

<div class="row">
  <div class="col-md-8">

  <% @questions.each do |question| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">

          <%= link_to question.id, question %>
        </h4>
      </div>
    </div>
  <% end %>
  </div>
</div>
